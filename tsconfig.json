{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@scripts/*": ["src/scripts/*"],
      "@data/*": ["src/data/*"],
      "@styles/*": ["src/styles/*"],
      "@models/*": ["src/models/*"],
      "@utils/*": ["src/utils/*"],
    },
  },
}
